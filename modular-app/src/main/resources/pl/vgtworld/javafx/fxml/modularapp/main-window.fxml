<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<BorderPane styleClass="main-window" fx:id="root" xmlns:fx="http://javafx.com/fxml" fx:controller="pl.vgtworld.javafx.fxml.modularapp.MainController">
    <top>
        <HBox spacing="5">
            <padding><Insets top="5" left="5" right="5" bottom="5"/></padding>
            <Button text="first" onAction="#firstPressed"/>
            <Button text="second" onAction="#secondPressed"/>
        </HBox>
    </top>
</BorderPane>